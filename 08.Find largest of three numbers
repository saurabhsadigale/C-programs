#include<stdio.h>
void main()
{
    int a,b,c;
    printf("\nEnter 1st number=");
    scanf("%d",&a);
    printf("\nEnter 2nd number=");
    scanf("%d",&b);
    printf("\nEnter 3rd number=");
    scanf("%d",&c);
     if(a>b&&a>c)
     {
         printf("\nLargest number is=%d",a);
     }
     else if(b>a&&b>c)
     {printf("\nLargest number is=%d",b);
}
else
{ printf("\nlargest number is=%d",c);
}
}
